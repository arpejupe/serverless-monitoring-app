{
  "name": "monitoring-tester-service",
  "version": "0.0.1",
  "private": true,
  "description": "Monitoring Service",
  "scripts": {
    "lint": "tslint 'src/**/*.ts'",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:ci": "JEST_JUNIT_OUTPUT=../../reports/junit/${npm_package_name}-test-results.xml yarn test --runInBand --ci",
    "test:e2e": "JEST_JUNIT_OUTPUT=../../reports/junit/${npm_package_name}-e2e-test-results.xml yarn test --runInBand --ci --config=jest.config.e2e.js",
    "coverage": "yarn test --coverage",
    "deploy": "serverless deploy",
    "remove": "serverless remove",
    "prettier": "prettier --write src/**/*.ts e2e/**/*.ts scripts/*.js",
    "prettier:ci": "prettier --list-different src/**/*.ts e2e/**/*.ts scripts/*.js",
    "print:name": "echo ${npm_package_name}"
  },
  "dependencies": {
    "aws-amplify": "^1.1.28",
    "aws-sdk": "^2.405.0",
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "@anttiviljami/serverless-stack-output": "^0.3.1",
    "@types/aws-lambda": "^8.10.19",
    "@types/chai": "^4.1.7",
    "@types/jest": "^23.3.2",
    "@types/mockdate": "^2.0.0",
    "@types/node": "^10.10.3",
    "aws-testing-library": "^1.0.0",
    "chai": "^4.2.0",
    "dotenv": "^6.2.0",
    "fs-extra": "^7.0.1",
    "jest": "23.6.0",
    "mockdate": "^2.0.2",
    "monitoring-appsync-service": "file:./../monitoring-appsync-service",
    "monitoring-common": "file:./../monitoring-common",
    "serverless-iam-roles-per-function": "^1.0.4",
    "serverless-plugin-tracing": "^2.0.0",
    "serverless-pseudo-parameters": "^2.4.0",
    "serverless-scriptable-plugin": "1.0.1",
    "serverless-step-functions": "^1.13.0",
    "serverless-webpack": "^5.2.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3",
    "webpack": "^4.29.5"
  },
  "author": "Erez Rokah",
  "license": "MIT"
}
